##endregion DAPS

    rdfs:isDefinedBy       nrd: ;
.

connector:http_host
    a             ids:ConnectorEndpoint ;
    ids:accessURL "https://alice.nicos-rd.com/" ;
.

connector:auth_info
    a                ids:AuthInfo ;
    ids:authService  <https://nrd-daps.nicos-rd.com/> ;
    ids:authStandard idsc:OAUTH2_JWT ;
.


@prefix ldp:            <http://www.w3.org/ns/ldp#>.
@prefix rdf:            <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:            <http://www.w3.org/2002/07/owl#> .
@prefix dct:            <http://purl.org/dc/terms/> .
@prefix foaf:           <http://xmlns.com/foaf/0.1/> .
@prefix xsd:            <http://www.w3.org/2001/XMLSchema#> .
@prefix org:            <http://www.w3.org/ns/org#> .

@prefix fua:            <https://www.nicos-rd.com.org/fua#> .
@prefix domain:         <https://www.nicos-rd.com.org/fua/domain#> .

@prefix nicos-ag:       <https://www.nicos-ag.com/> .
@prefix nrd:            <https://www.nicos-rd.com/> .

@prefix ecm:            <https://www.nicos-rd.com.org/ecosystem#> .
@prefix tbm:            <https://www.nicos-rd.com.org/testbed#> .

@base <https://testbed.nicos-rd.com/> .
@prefix tb:             <https://testbed.nicos-rd.com/> .
@prefix ts:             <https://testbed.nicos-rd.com/testsuite/> .

tb:
    a                tbm:Testbed ;
    rdfs:label       "nrd Testbed" ;
#    foaf:homepage                           "https://testbed.nicos-rd.com/browse" ;
## region meta
    ecm:owner        nrd: ;
    ecm:provider     nrd: ;
    ecm:domain       [ a domain:Domain ; ] ;
## OR ?!?
    domain:domain    [ a domain:Domain ; ] ;
## endregion meta
## region configuration
## region configuration application
    fua:port         "80"^^xsd:nonNegativeInteger ; ## default = 80
## endregion configuration application
    ecm:ecosystem    <https://testbed.nicos-rd.com/ec/ids/> ;
    tbm:testsuite    ts: ; ## 1..n
    owl:sameAs       <https://10.10.33.42> ;
    owl:sameAs       <https://testbed.nicos-rd.com/> ;
## region configuration applicant
    tbm:applicant    <https://nicos-gbx.nicos-rd.com/> ;
## endregion configuration applicant
## endregion configuration
    rdfs:isDefinedBy nrd: ;
.

tb:domain
    a                domain:Domain ;
#    domain:users    <https://testbed.local:8080/users/jlangkau> ;
#    domain:users     [ a            domain:Users ;
#                       ldp:contains ( <https://testbed.local:8080/users/jlangkau> ) ] ;
    domain:users     tb:Users ;
    rdfs:isDefinedBy nrd: ;
.

tb:Users
    a                domain:Users ;
    ldp:contains     ( <https://testbed.nicos-rd.com/domain/users/testsuite>
                       ##  Order ALPHA
                       <https://testbed.nicos-rd.com/domain/users/jlangkau>
                       <https://testbed.nicos-rd.com/domain/users/spetrac> ) ;
    rdfs:isDefinedBy nrd: ;
.

ts:
    a                tbm:Testsuite ;
    rdfs:label       "nrd Testsuite" ;
#    foaf:homepage                           "https://testbed.nicos-rd.com/testsuite/browse" ;
## region meta
    ecm:owner        nrd: ;
    ecm:provider     nrd: ;
    domain:domain    [ a domain:Domain ; ] ;
## endregion meta
## region configuration
## region configuration application
    fua:port         "80"^^xsd:nonNegativeInteger ; ## default = 80
## endregion configuration application
    ecm:ecosystem    <https://testbed.nicos-rd.com/ec/ids/> ;

    tbm:testbed      tb: ; ## 0..1
    owl:sameAs       <https://10.10.33.43> ;
    owl:sameAs       <https://testbed.nicos-rd.com> ;
## region configuration applicant
    tbm:applicant    <https://nicos-gbx.nicos-rd.com/> ;
## endregion configuration applicant
## endregion configuration
    rdfs:isDefinedBy nrd: ;
.

## EOF : nrd-testbed

agent:
    a          ids:Participant ;
## comment : 'org:Organization' is present in ids:Participant, also. So, if agent is not a ids:Participant
##           we can use 'org:Organization' (or 'foaf:Person') instead
    a          org:Organization ;
    rdfs:label "nicos Research & Development GmbH"^^xsd:string ;
.

